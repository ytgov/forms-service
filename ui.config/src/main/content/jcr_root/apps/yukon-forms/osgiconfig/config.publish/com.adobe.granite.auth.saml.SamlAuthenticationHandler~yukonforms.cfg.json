{
  "path": [
    "/content/yukon-forms"
  ],
  "idpCertAlias": "$[env:SAML_IDP_CERT_ALIAS]",
  "identitySyncType": "idp_dynamic_simplified_id",
  "idpIdentifier": "yukonforms",
  "idpUrl": "$[env:SAML_IDP_URL]",
  "handleLogout": false,
  "logoutUrl": "$[env:SAML_IDP_LOGOUT]",
  "serviceProviderEntityId": "$[env:SAML_ENTITY_ID]",
  "useEncryption": false,
  "userIDAttribute": "",
  "createUser": true,
  "userIntermediatePath": "yukon/sso",
  "nameIdFormat": "urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress",
  "synchronizeAttributes": [
    "Email=profile/email",
    "FirstName=profile/givenName",
    "LastName=profile/familyName"
  ],
  "addGroupMemberships": true,
  "groupMembershipAttribute": "groups",
  "defaultGroups": [
    "yukon-members"
  ]
}
